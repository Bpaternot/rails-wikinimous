<h1>List of articles</h1>
<%= link_to "Create", new_article_path, method: :get%>
<% @articles.each do |article| %>
<h2><%= link_to article.title, article_path(article)%></h2>
<p><%= article.content %></p>
<p><%= link_to "Edit", edit_article_path(article), method: :get%></p>
<p><%= link_to "Delete", article_path(article), method: :delete%></p>
<p>__________________________________________________________</p>
<% end %>
